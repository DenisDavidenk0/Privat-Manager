{
	"name": "vite-react-boilerplate",
	"author": "Ricardo Valdovinos <ricardovaldovinos1996@gmail.com>",
	"description": "A production ready, batteries included starter template for Vite + React projects",
	"keywords": [
		"vite",
		"react",
		"boilerplate",
		"starter",
		"template"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/RicardoValdovinos/vite-react-boilerplate"
	},
	"license": "MIT",
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"setup": "git init && npx husky init && shx rm .husky/pre-commit",
		"format": "prettier src/**/*.{ts,tsx} --write",
		"lint": "eslint --max-warnings 0",
		"lint:fix": "eslint --max-warnings 0 --fix",
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"commitlint": "commitlint --edit",
		"commitizen": "exec < /dev/tty && git cz --hook || true",
		"prepare": "husky"
	},
	"dependencies": {
		"@emotion/react": "^11.13.3",
		"@emotion/styled": "^11.13.0",
		"@hookform/resolvers": "^3.9.0",
		"@mui/icons-material": "^6.1.2",
		"@mui/material": "^6.1.2",
		"@mui/toolpad": "^0.1.51",
		"dayjs": "^1.11.13",
		"dotenv": "^16.4.5",
		"ky": "^1.7.2",
		"react": "^18.3.1",
		"react-admin": "^5.2.2",
		"react-dom": "^18.3.1",
		"react-router": "^6.26.2",
		"react-router-dom": "^6.26.2",
		"recharts": "^2.12.7",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.4.1",
		"@commitlint/config-conventional": "^19.4.1",
		"@eslint/compat": "^1.1.1",
		"@eslint/js": "^9.9.1",
		"@playwright/test": "^1.46.1",
		"@tanstack/react-query-devtools": "^5.53.1",
		"@tanstack/react-table-devtools": "^8.20.5",
		"@testing-library/jest-dom": "^6.5.0",
		"@testing-library/react": "^16.0.1",
		"@testing-library/user-event": "^14.5.2",
		"@total-typescript/ts-reset": "^0.6.0",
		"@types/eslint__js": "^8.42.3",
		"@types/node": "^22.5.1",
		"@types/react": "^18.3.5",
		"@types/react-dom": "^18.3.0",
		"@typescript-eslint/eslint-plugin": "^8.3.0",
		"@typescript-eslint/parser": "^8.3.0",
		"@vitejs/plugin-react-swc": "^3.7.0",
		"autoprefixer": "^10.4.20",
		"commitizen": "^4.3.0",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.57.1",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.30.0",
		"eslint-plugin-jsx-a11y": "^6.9.0",
		"eslint-plugin-react": "^7.35.0",
		"eslint-plugin-react-hooks": "^4.6.2",
		"eslint-plugin-react-refresh": "^0.4.11",
		"eslint-plugin-storybook": "^0.8.0",
		"eslint-plugin-unicorn": "^55.0.0",
		"globals": "^15.9.0",
		"husky": "^9.1.5",
		"jsdom": "^25.0.0",
		"postcss": "^8.4.41",
		"prettier": "^3.3.3",
		"prop-types": "^15.8.1",
		"shx": "^0.3.4",
		"typescript": "^5.5.4",
		"typescript-eslint": "^8.3.0",
		"vite": "5.4.2",
		"vite-plugin-static-copy": "^1.0.6"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
